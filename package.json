{
  "name": "@sil/tools",
  "version": "0.1.12",
  "description": "Sil's Toolbelt",
  "main": "src/index.ts",
  "module": "dist/es2015/lib/index.js",
  "scripts": {
    "test": "jest",
    "build": "npm run build:es2015 && npm run build:mjs && npm run build:esm && npm run build:cjs",
    "build:mjs": "tsc --module esnext --target esnext --outDir dist/mjs",
    "build:es2015": "tsc --module es2015 --target es2015 --outDir dist/es2015",
    "build:esm": "tsc --module es2015 --target es5 --outDir dist/esm",
    "build:cjs": "tsc --module commonjs --target es5 --outDir dist/cjs",
    "dev": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/silvandiepen/tools.git"
  },
  "keywords": [
    "Sil's",
    "JS",
    "Tools"
  ],
  "author": "Sil van Diepen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/silvandiepen/tools/issues"
  },
  "homepage": "https://github.com/silvandiepen/tools#readme",
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.1",
    "babel-jest": "^27.4.6",
    "ts-jest": "^27.1.2",
    "typescript": "^4.6.0-dev.20220207"
  },
  "dependencies": {
    "jest": "^27.4.7"
  }
}
